{
  "name": "terracotta-jewelry-frontend",
  "version": "1.0.0",
  "description": "Terracotta Jewelry Store - Handcrafted ceramic jewelry",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "build": "vite build",
    "build:check": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist",
    "generate-client": "node generator/generate-client.js",
    "deploy": "npm run build && netlify deploy --prod --dir=dist --site=fe2b396b-a5fb-4f0a-94bc-fd9877c89c19 --skip-functions-cache",
    "deploy:preview": "npm run build && netlify deploy --dir=dist --site=fe2b396b-a5fb-4f0a-94bc-fd9877c89c19 --skip-functions-cache",
    "deploy:simple": "npm run build && cd dist && netlify deploy --prod --dir=. --site=fe2b396b-a5fb-4f0a-94bc-fd9877c89c19"
  },
  "dependencies": {
    "@ecommerce/shared-services": "*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^6.5.0",
    "@mui/icons-material": "^6.5.0",
    "zustand": "^4.5.5",
    "zod": "^3.23.8",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.2.0",
    "typescript": "~5.7.2",
    "eslint": "^9.21.0",
    "@eslint/js": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "typescript-eslint": "^8.24.1",
    "globals": "^15.15.0"
  }
}